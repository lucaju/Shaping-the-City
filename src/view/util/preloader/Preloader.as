package view.util.preloader {		//import	import flash.display.Loader;	import flash.display.Sprite;	import flash.net.URLRequest;		public class Preloader extends Sprite {		/* Variable to store dataTextField param in the constructor function */				private var animation:Loader;				/* Start function */				public function start():void {						var urlRequest:URLRequest = new URLRequest("view/util/preloader/preloader.swf")			animation = new Loader();			animation.load(urlRequest);			this.addChild(animation);						animation.scaleX = animation.scaleY = .1;		}			}}